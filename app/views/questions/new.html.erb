<h1>Questions#new</h1>
<p>Find me in app/views/questions/new.html.erb</p>

<%= form_for @question do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :body %>
  <%= f.text_area :body %>

  <%= f.label :resolved %>
  <%= f.check_box :resolved %>

  <%= f.submit "Save" %>
<% end %>
